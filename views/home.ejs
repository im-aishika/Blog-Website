<%- include("partials/header") -%>

     <h1>Home</h1>
     <p><%= homeContent %> </p>
     <hr>

     <% posts.forEach(function(element) { %>
        <h1> <%= element.title %></h1>
        <p>
          <%= element.content.substring(0, 200) + "..." %>
          <a href = "/post/<%=element.title %>">Read More</a>
          <hr>
        </p>
     <% })  %>

<%- include("partials/footer"); -%>
